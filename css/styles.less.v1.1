// Variables
@text-color:		white;
@link-color:        white;
@link-color-hover:  darken(@link-color, 50%);
@letter-spacing-geral: 	.1px;
@background-geral: url(../assets/bg-geral.jpg);
@bordernav: 1px solid white;
@nav-height: 40px;
@max-width-wrapper:66vh;
@roxoescuro: #500026;
@bodywrapper__borda: 1px solid rgba(80, 0, 38, 0.50);

// Usage


body {
	background-image: @background-geral;
	background-size: cover;
	font-family: 'Raleway';
	color: @text-color;
	letter-spacing: @letter-spacing-geral;
	padding-top: @nav-height;
}


.sgnav-top {
	border-bottom: @bordernav;
	padding: 5px;
	position: fixed;
	top: 0;
	z-index: 999999;
	width: 100%;
	height: @nav-height;
	background: @background-geral no-repeat center ;
	background-size: cover
}

.sgnav-top .rioexxlogo {
	background-image: url('../assets/logo-re-h.png');
	height: 100%;
	width: 45%;
	background-size: contain;
	background-repeat: no-repeat;
	background-position: center;
}

.sgnav-top .infoevendas {
	text-align: right;
	line-height: 1em;
	font-size: .80em;
	font-weight: 600;
}

.sgnav-top .infoevendastext {
	display: block;
	font-weight: 900;
}

a,
.link {
	color: @link-color;
}
a:hover,
a:visited {
	color: @link-color;
	text-decoration: none;
}

.icon-link, .icon-link:hover {
	color: white;
	font-size: 1.5em;
}

p {
	margin-bottom: 10px;
}

button:hover {
	cursor: pointer;
}

/* Menu */

.navbar-header {
	float: none;
}

.sgnav-bottom {
	position: fixed;
	width: 100%;
	padding: 8px;
	bottom: 0;
	background: @background-geral no-repeat center ;
	background-size: cover;
	z-index: 99999;
	height: @nav-height;
	border-top: @bordernav;
}

.sgnav-bottom .container-fluid {
	padding: 0;
}

.sgnav-bottom button {
	outline-color: transparent !important;
}

.vendas {
	margin-bottom:-.5em;
}

.compre-style {
	font-weight: 600;
	font-size: 1em;
	display: block;
	line-height: .5em;
}

.agora-style {
	font-weight: bolder;
	font-size: 1.2em;
	line-height: .5em;
}

.modal-fullscreen-menu {

	.modal-dialog {
		margin: 0 auto;
		width: 100%;
		max-width: 66vh;
		display: flex;
		height: 100%;
		align-items: center;
		padding-bottom: 40px;
	}

	.modal-content {
		background-image: @background-geral;
		box-shadow: none;
		border: none;
		height: 100%;
		width: 100%;
		border-radius: 0;
		border-left: 1px solid transparent;
		border-right: 1px solid transparent;
	}

	.list-group {
		top: 50%;
		position: absolute;
		-webkit-transform: translateY(-50%);
		        transform: translateY(-50%);
		text-align: left;
		padding-top: @nav-height;
		width: 100%;
		a {
			font-weight: 700;
			letter-spacing: 0.05em;
			border: none;
			transition: all 0.25s ease;
			background-color: transparent;
			color: white;
			font-size: 3vh;
			padding: 1.2%;
			padding-left: 8%;
			font-style: italic;
			letter-spacing: -0.5px;
			&:nth-child(odd) {
				background-color: rgba(0, 0, 0, 0.03);
			}
			&:before {
				content: '';
				position: absolute;
				top: 0;
				left: 0;
				width: 100%;
				height: 100%;
				background: white;
				z-index: -1;
				opacity: 0;
				-webkit-transform: scale3d(0.7, 1, 1);
				        transform: scale3d(0.7, 1, 1);
				transition: opacity 0.4s, -webkit-transform 0.4s;
				transition: transform 0.4s, opacity 0.4s;
				transition: transform 0.4s, opacity 0.4s, -webkit-transform 0.4s;
			 }

			&:hover {
				font-weight: 900;
				color: #500026;

				&:before {
					-webkit-transform: translate3d(0, 0, 0);
					        transform: translate3d(0, 0, 0);
					opacity: 1;
				}
			}
		}
	}
}

.modal-backdrop {
	&.in {
		opacity: 1;
	}
}

/*HAMBURGER*/

@hamburger-width: 40px;

.hamburger {
	padding: 0 10px;
}

.hamburger-box {
	bottom:-2px;
}
.hamburger--collapse .hamburger-inner::after {
	top: -16px;
}

.hamburger-inner::before {
	top: -8px;
}

.hamburger-inner, .hamburger-inner::before, .hamburger-inner::after {
	width: @hamburger-width;
	height: 2px;
}

.hamburger-inner, .hamburger-inner::before, .hamburger-inner::after, .hamburger.is-active .hamburger-inner, .hamburger.is-active .hamburger-inner::before, .hamburger.is-active .hamburger-inner::after {
	background-color: white;
}

.hamburger-box {
	width: @hamburger-width;
}

