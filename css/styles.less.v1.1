	.programacao-title {
		position: relative;
		z-index: 1;
	}
	.swiper-button-prev, .swiper-button-next {
		filter: grayscale(100%) brightness(4) contrast(1);
		height: 30px;
		width: 30px;
		margin-top: -60px;
		&:after {
			width: 100px;
			color: white;
			top: 30px;
			position: relative;
			white-space: pre;
			font-weight: 600;
			font-size: 13px;
			font-size: 4.0625VW;	
		}
	}
	.swiper-button-prev::after {
		content: '1° andar';
	}
	.swiper-button-next::after {
		content: '2° andar';
		left: -7vw;
	}
	.swiper-button-disabled {
		display: none;
	}
}

