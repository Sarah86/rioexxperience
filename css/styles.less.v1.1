/*Flexbox Centering*/

.flexbox-centering {
	display: flex;
	justify-content: center;
	align-items: center;
	height: 100px;
}

/*Transform centering*/

.transform-centering {
	left: 50%;
	position: absolute;
	top: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	text-align: center;
}

/*Transform horizontal centering*/
.transform-h-centering {
	left: 50%;
	position: absolute;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	text-align: center;
}

